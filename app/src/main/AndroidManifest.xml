<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="andtrain.com.androidtraining_booking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.UserLandingHomePageActivity">
            <intent-filter>
                <action android:name="andtrain.com.androidtraining_booking.activities.UserLandingHomePageActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity">
            <intent-filter>
                <action android:name="andtrain.com.androidtraining_booking.activities.LoginActivity" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ResultActivity" android:label="@string/title_activity_result">
            <!--<meta-data android:name="android.support.PARENT_ACTIVITY" android:value=".MainActivity"></meta-data>-->
            <intent-filter>
                <action android:name="andtrain.com.androidtraining_booking.activities.ResultActivity"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
